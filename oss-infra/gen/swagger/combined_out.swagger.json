          "title": "无权限时的原因"
        }
      }
    },
    "fileFileInfo": {
      "type": "object",
      "properties": {
        "file_id": {
          "type": "string"
        },
        "filename": {
          "type": "string"
        },
        "file_key": {
          "type": "string"
        },
        "file_size": {
          "type": "string",
          "format": "int64"
        },
        "mime_type": {
          "type": "string"
        },
        "category": {
          "type": "string"
        },
        "owner_id": {
          "type": "string"
        },
        "visibility": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "download_url": {
          "type": "string",
          "title": "临时下载URL"
        }
      },
      "title": "文件信息"
    },
    "fileGetDownloadUrlResp": {
      "type": "object",
      "properties": {
        "download_url": {
          "type": "string"
        },
        "expires_in": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "fileGetFileInfoResp": {
      "type": "object",
      "properties": {
        "file_info": {
          "$ref": "#/definitions/fileFileInfo"
        }
      }
    },
    "fileListFilesResp": {
      "type": "object",
      "properties": {
        "files": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/fileFileInfo"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        },
        "page": {
          "type": "integer",
          "format": "int32"
        },
        "page_size": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "fileUploadFileReq": {
      "type": "object",
      "properties": {
        "file_data": {
          "type": "string",
          "format": "byte",
          "title": "文件数据"
        },
        "filename": {
          "type": "string",
          "title": "文件名"
        },
        "category": {
          "type": "string",
          "title": "文件分类: avatar, product, document"
        },
        "visibility": {
          "type": "string",
          "title": "可见性: public, private"
        }
      },
      "title": "上传文件请求"
    },
    "fileUploadFileResp": {
      "type": "object",
      "properties": {
        "file_info": {
          "$ref": "#/definitions/fileFileInfo"
        }
      }
    }
  },
  "securityDefinitions": {
    "ApiKeyAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
