{
  "swagger": "2.0",
  "info": {
    "title": "proto/inventory/payment/payment.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "PaymentService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/v1/payments": {
      "post": {
        "summary": "创建支付订单",
        "description": "创建支付订单并返回支付参数",
        "operationId": "PaymentService_CreatePaymentOrder",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/paymentCreatePaymentOrderResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/paymentCreatePaymentOrderReq"
            }
          }
        ],
        "tags": [
          "PaymentService"
        ]
      }
    },
    "/api/v1/payments/callback/{provider}": {
      "post": {
        "summary": "支付回调处理",
        "description": "处理第三方支付平台的回调通知",
        "operationId": "PaymentService_HandlePaymentCallback",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/paymentHandlePaymentCallbackResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "provider",
            "description": "支付提供商 (alipay/wechat)",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/PaymentServiceHandlePaymentCallbackBody"
            }
          }
        ],
        "tags": [
          "PaymentService"
        ]
      }
    },
    "/api/v1/payments/{paymentId}": {
      "get": {
        "summary": "查询支付订单",
        "description": "根据支付订单ID查询支付状态",
        "operationId": "PaymentService_GetPaymentOrder",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/paymentGetPaymentOrderResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "paymentId",
            "description": "支付订单ID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "PaymentService"
        ]
      }
    },
    "/api/v1/payments/{paymentId}/refunds": {
      "post": {
        "summary": "申请退款",
        "description": "对已支付的订单申请退款",
        "operationId": "PaymentService_CreateRefund",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/paymentCreateRefundResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "paymentId",
            "description": "支付订单ID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/PaymentServiceCreateRefundBody"
            }
          }
        ],
        "tags": [
          "PaymentService"
        ]
      }
    },
    "/api/v1/refunds/{refundId}": {
      "get": {
        "summary": "查询退款状态",
        "description": "根据退款ID查询退款状态",
        "operationId": "PaymentService_GetRefund",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/paymentGetRefundResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "refundId",
            "description": "退款ID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "PaymentService"
        ]
      }
    }
  },
